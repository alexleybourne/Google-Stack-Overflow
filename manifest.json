{
    "name": "Google Stack Overflow",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "description": "Skip the middleman",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
        {
          "matches": ["https://www.google.com/*"],
          "js": ["script.js"],
          "css": ["styles.css"]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}